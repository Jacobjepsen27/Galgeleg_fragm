<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jepsen.dk.galgeleg_fragm.highscore_viewTemp">


    <LinearLayout
        android:id="@+id/highscore_view_temp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_column="0">


        <fragment
            android:layout_height="0px"
            android:layout_weight="8"
            android:layout_width="match_parent"
            android:name="jepsen.dk.galgeleg_fragm.highscore_temp"
            android:id="@+id/fragment4"
            android:layout_gravity="center_horizontal"
            tools:layout="@layout/fragment_highscore_temp"/>

        <fragment
            android:layout_height="0px"
            android:layout_weight="2"
            android:layout_width="match_parent"
            android:name="jepsen.dk.galgeleg_fragm.Spil_igen"
            android:id="@+id/fragment3"
            android:layout_gravity="center_horizontal"
            tools:layout="@layout/fragment_spil_igen"/>
    </LinearLayout>


</RelativeLayout>
